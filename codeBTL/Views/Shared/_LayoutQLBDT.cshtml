<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Quản lý bán điện thoại di động</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@300;400;500;600;700;800;900&family=Roboto:wght@300;400;500;700&display=swap"
          rel="stylesheet" />

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
          crossorigin="anonymous" />
    <link rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css" />

    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css"
          integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer" />

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
            crossorigin="anonymous"></script>
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" />
    <!-- [QUAN] Trang dien thoai + Phu kien + chi tiet dien thoai  -->
    <link rel="stylesheet"
          href="~/assets/css/products/bootstrapv3.min.css" />
    <link rel="stylesheet"
          href="~/assets/css/products/chitietdienthoai.css" />
    <link rel="stylesheet"
          href="~/assets/css/products/color-swatch.css" />
    <link rel="stylesheet"
          href="~/assets/css/products/owl.carousel.css" />
    <link rel="stylesheet"
          href="~/assets/css/style.css" />
    <script type="text/javascript" src="~/assets/js/products/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/assets/js/products/bootstrap.min.js"></script>
    <!-- QUAN END  -->
    <!-- CSS của SweetAlert2 -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@10.16.3/dist/sweetalert2.min.css">

    <!-- JavaScript của SweetAlert2 -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10.16.3/dist/sweetalert2.all.min.js"></script>


</head>
<body>
    <!-- begin header -->
    @await Html.PartialAsync("_PartialHeader")
    <!-- end header -->
    <!-- begin nav -->
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <!-- Container wrapper -->
        <div class="container-fluid">
            <!-- Toggle button -->
            <button class="navbar-toggler"
                    type="button"
                    data-mdb-toggle="collapse"
                    data-mdb-target="#navbarCenteredExample"
                    aria-controls="navbarCenteredExample"
                    aria-expanded="false"
                    aria-label="Toggle navigation">
                <i class="fas fa-bars"></i>
            </button>

            <!-- Collapsible wrapper -->
            <div class="navbar-collapse justify-content-center"
                 id="navbarCenteredExample">
                <!-- Left links -->
                <ul class="navbar-nav mb-2 mb-lg-0">
                    <li class="nav-item">

                        <a asp-controller="Home" asp-action="Index" class="nav-link active"
                           aria-current="page">Trang chủ</a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="Smartphone" class="nav-link"
                           aria-current="page">Điện thoại</a>
                    </li>
                    <!-- Navbar dropdown -->
                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="PhoneAccessories" class="nav-link"> Phụ kiện điện thoại </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="About" class="nav-link"> Giới thiệu </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="News" class="nav-link"> Tin tức </a>
                    </li>

                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="Contacts" class="nav-link"> Liên hệ </a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Home" asp-action="Feedback" class="nav-link"> Phản hồi </a>
                    </li>

                    <li class="nav-item">
                        <a asp-controller="Access" asp-action="Logout" class="nav-link"><i class="bx bx-log-out bx-rotate-180"></i> Đăng xuất</a>
                    </li>
                </ul>
                <!-- Left links -->
            </div>
            <!-- Collapsible wrapper -->
        </div>
        <!-- Container wrapper -->
    </nav>
    <!-- end nav -->
    <!-- begin main -->
    <main>
        <div class="container_cus">
            @RenderBody()
        </div>
    </main>
    <!-- end main -->
    <!-- begin footer -->
    @await Html.PartialAsync("_PartialFooter")
    <!-- end footer -->
</body>
<link rel="stylesheet" href="~/assets/css/style.css" />
<link rel="stylesheet" href="~/assets/css/main.css" />
<link rel="stylesheet" href="~/assets/css/products/bootstrapv3.min.css" />
<script src="//cdn.jsdelivr.net/npm/sweetalert2@10"></script>
</html>
